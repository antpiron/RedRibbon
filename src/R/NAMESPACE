useDynLib(RedRibbon)

import(ggplot2)

S3method(newRRHO, data.frame)
S3method(newRRHO, numeric)
S3method(ggplot, rrho)
S3method(setoptions, rrho)

export("rrho_rectangle", "rrho_rectangle_min", "rrho_rectangle_min_ea", "rrho_ij",
       "newRRHO", "setoptions", "rectangle_min",
       "newRRHO.data.frame", "newRRHO.numeric",
       "ggplot.rrho",
       "setoptions.rrho",
       "rectangle_min.rrho")
