##useDynLib(RedRibbon, rrho_r_rectangle)
useDynLib(RedRibbon)

import(ggplot2)

S3method(newRRHO, data.frame)
S3method(newRRHO, numeric)
S3method(ggplot, rrho)
S3method(setoptions, rrho)
S3method(rectangle_min, rrho)
S3method(quadrants, rrho)

export("rrho_rectangle", "rrho_rectangle_min", "rrho_rectangle_min_ea", "rrho_permutation", "rrho_ij", "rrho_intersect",
       "newRRHO", "setoptions", "rectangle_min", "enrichment", "quadrants",
       "newRRHO.data.frame", "newRRHO.numeric",
       "ggplot.rrho", "setoptions.rrho", "rectangle_min.rrho", "enrichment.rrho", "quadrants.rrho")
